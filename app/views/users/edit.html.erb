<% if @user.errors.any? %>
  <%= react_component("ErrorMessages", { messages: @user.errors.full_messages }) %>
<% end %>
<%= react_component("UserEdit", { user: @user, isCurrentUser: current_user?(@user), token: form_authenticity_token(form_options: { action: user_path(@user), method: "patch" }) }, { style: "height:calc(100% - 56px)" } ) %>
